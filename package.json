    "stack-chain": "^1.3.0",
    "tape": "^3.0.3",
    "which": "^1.0.8"
  },
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jscs": "^1.9.0",
    "jshint": "^2.5.11",
    "pre-commit": "0.0.9",
    "tape": "^3.0.3"
  },
  "keywords": [
    "errors",
    "format",
    "stack"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
